# Allow after '{', ':', and ',', and before '}'.
def map = {

"a":

"a value",

"b": "b value"

}

IO.println(map["a"]) # expect: a value
IO.println(map["b"]) # expect: b value

# Newline after trailing comma.
map = {"c": "c value",

}

IO.println(map["c"]) # expect: c value

# Newline in empty map.
map = {

}

IO.println(map.size) # expect: 0
