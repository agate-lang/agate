def x = 2.55e2

IO.println(x) # expect: 255
IO.println(x + 1.0) # expect: 256
IO.println(x == 255.0) # expect: true
IO.println(2.55e-2 is Float) # expect: true
IO.println(x is Float) # expect: true
IO.println(-2.55e2) # expect: -255
IO.println(-25500e-2) # expect: -255
IO.println(2.55e+2) # expect: 255
IO.println(2e+2) # expect: 200
