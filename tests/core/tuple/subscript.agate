def tuple = (1, nil, true, 3.0, 'a')

IO.println(tuple[0]) # expect: 1
IO.println(tuple[1]) # expect: nil
IO.println(tuple[2]) # expect: true
IO.println(tuple[3]) # expect: 3
IO.println(tuple[4]) # expect: a

IO.println(tuple[-5]) # expect: 1
IO.println(tuple[-4]) # expect: nil
IO.println(tuple[-3]) # expect: true
IO.println(tuple[-2]) # expect: 3
IO.println(tuple[-1]) # expect: a
