IO.println(123 == 123)  # expect: true
IO.println(123 == 124)  # expect: false
IO.println(-3 == 3)     # expect: false
IO.println(0 == -0)     # expect: true

# Not equal to other types.
IO.println(123 == "123") # expect: false
IO.println(1 == true)    # expect: false
IO.println(0 == false)   # expect: false

IO.println(123 != 123)  # expect: false
IO.println(123 != 124)  # expect: true
IO.println(-3 != 3)     # expect: true
IO.println(0 != -0)     # expect: false

# Not equal to other types.
IO.println(123 != "123") # expect: true
IO.println(1 != true)    # expect: true
IO.println(0 != false)   # expect: true
